# Configuração otimizada para Render Free
NODE_ENV=production
RENDER=true

# Desabilitar logs de debug
DEBUG=false

# Otimizações específicas do Render
# Reduzir timeout de comandos para evitar sleep do container
COMMAND_TIMEOUT=15000

# Configurações de memória
NODE_OPTIONS="--max-old-space-size=512"

# Rate limiting mais agressivo para economizar recursos
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=10

# Configurações do Discord
DISCORD_TOKEN=your_token_here
CLIENT_ID=your_client_id_here

# Configurações do banco (AWS RDS)
DB_HOST=your_db_host
DB_USER=your_db_user  
DB_PASSWORD=your_db_password
DB_NAME=your_db_name
DB_PORT=3306

# Configurações do admin panel
ADMIN_PASSWORD=admin123
